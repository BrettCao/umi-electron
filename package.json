{
  "name": "little-strong",
  "version": "1.0.0",
  "description": "an example based on umijs + electron",
  "main": "./dist/main/main.js",
  "private": true,
  "build": {
    "productName": "LittleStrong",
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "release"
    },
    "appId": "com.cn.littlestrong.demo",
    "asar": false
  },
  "scripts": {
    "start": "npm run build-main-dev && run-electron ./dist/main/main.js",
    "start:main": "electron-webpack dev",
    "start:renderer": "cross-env APP_ROOT=src/renderer umi dev",
    "build-main-prod": "NODE_ENV=production webpack --config ./build/webpack.main.prod.config.js",
    "build-main-dev": "NODE_ENV=development webpack --config ./build/webpack.main.config.js",
    "build:renderer": "cross-env APP_ROOT=src/renderer umi build",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "debug:main": "run-electron --inspect=5858 -w ./dist/main/main.js",
    "pack": "npm run build:renderer && npm run build-main-prod && electron-builder --dir",
    "exe": "electron-builder --win",
    "dist": "electron-builder",
    "prettier": "prettier --list-different \"./**/*.{ts,tsx,js,jsx,less}\""
  },
  "keywords": [
    "Electron",
    "umi",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "devDependencies": {
    "cross-env": "^5.1.1",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-webpack": "^2.7.4",
    "prettier": "1.17.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "umi": "^2.8.16",
    "umi-plugin-dll": "^1.5.2",
    "umi-plugin-dva": "^1.7.7",
    "umi-plugin-react": "^1.9.14",
    "umi-plugin-routes": "^1.4.6",
    "webpack": "4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "antd": "^3.21.2",
    "axios": "^0.19.0",
    "braft-editor": "^2.3.7",
    "braft-extensions": "0.0.18",
    "braft-utils": "^3.0.12",
    "classnames": "^2.2.6",
    "dva": "^2.5.0-beta.2",
    "dva-model-extend": "^0.1.2",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "is-empty": "^1.2.0",
    "js-md5": "^0.7.3",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "node.extend": "^2.0.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "rc-tree": "^1.15.2",
    "react": "^16.7.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-iframe-comm": "^1.2.2",
    "run-electron": "^1.0.0"
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/main"
    },
    "renderer": {
      "sourceDirectory": null
    }
  }
}
